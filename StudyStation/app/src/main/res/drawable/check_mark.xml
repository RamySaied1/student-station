<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="20dp"
        android:height="3dp"
        android:top="28dp"
        android:left="7dp">
        <rotate
            android:fromDegrees="80">
            <shape android:shape="rectangle">
                <solid android:color="#14c70b"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="4dp"
        android:top="20dp"
        android:left="10dp"
        >
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#14c70b"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="50dp"
        android:height="50dp">
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
        </shape>
    </item>
</layer-list>